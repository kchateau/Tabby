{
  "manifest_version": 2,
  "name": "Tabby",
  "description": "Tab Organizer",
  "version": "1.0.1",

  "icons": { 
    "48": "icon.png"
   },

  "permissions": [
    "activeTab",
    "storage"
    ],

  "browser_action": {
    "default_title": "Tabby",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}